using System;

class Program
{
    static void Main(string[] args)
    {
        var n = Convert.ToInt32(Console.ReadLine());
        var array = Array.ConvertAll(Console.ReadLine().Split(' '), s => int.Parse(s));
        for (int i = array.Length; i >= 1; i--)
        {
            Console.Write("{0} ", array[i-1]);
        }
    }
}
