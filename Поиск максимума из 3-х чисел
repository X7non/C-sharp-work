using System;

namespace Test
{
    class Program
    {
        static void Main(string[] args)
        {
            double a, b, c; //определение переменных
            a = Convert.ToDouble(Console.ReadLine()); //ввод
            b = Convert.ToDouble(Console.ReadLine());
            c = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine(Math.Max(a, Math.Max(b, c))); // определение максимума
        }
    }
}
