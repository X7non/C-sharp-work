using System;
class Program
{
    static void Main(string[] args)
    {
        string a = Console.ReadLine();
        a = a.Replace(" ", "");
        a = a.ToLower();
        string k = "";
        for (int i = a.Length - 1; i >= 0; i--)
        {
            k += a[i];
        }
        if (k == a)
        {
            Console.WriteLine("Yes");
        } else
        {
            Console.WriteLine("No");
        }
    }
}
