using System;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int n, s = 0, k=0;
            n = Convert.ToInt32(Console.ReadLine());
            for (int i = 2; i<=n; i=i+2)
            {
                s += i;
                k++;
            }
            Console.WriteLine("{0} Ð¸ {1}", s, k);
        }
    }
}
