using System;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int a;
            var s = "*";
            a = Convert.ToInt32(Console.ReadLine());
            for (int i = 1; i<=a; i++)
            {
                for (int j=1; j<=i; j++)
                {
                    Console.Write(s);
                }
                Console.WriteLine();
            }
        }
    }
}
